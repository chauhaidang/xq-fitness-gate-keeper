{
  "name": "@chauhaidang/gate-keeper",
  "version": "1.0.0",
  "description": "API automation testing framework - install globally with npm",
  "main": "index.js",
  "bin": {
    "xq-keeper": "./bin/run-tests.js"
  },
  "preferGlobal": true,
  "files": [
    "bin",
    "tests",
    "fixtures",
    "playwright.config.ts",
    "tsconfig.json",
    "generated-clients"
  ],
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report"
  },
  "keywords": [
    "playwright",
    "api-testing",
    "e2e",
    "workflow-testing",
    "test-automation",
    "cli",
    "global-install"
  ],
  "author": "XQ Fitness Team",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.40.0",
    "xq-fitness-write-client": "file:./generated-clients/write-service",
    "xq-fitness-read-client": "file:./generated-clients/read-service"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "dotenv": "^17.2.3",
    "typescript": "^5.5.4"
  }
}
